    "grunt-aws": "^0.5.4",
    "grunt-bump": "0.3.1",
    "grunt-checkbranch": "^1.0.2",
    "grunt-checkrepo": "^0.1.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^0.13.0",
